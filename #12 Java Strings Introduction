import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        /* Enter your code here. Print output to STDOUT. */
        
        int l1 = A.length();
        int l2 = B.length();
        System.out.println(l1+l2);
        if(A.compareTo(B)>0){
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }
        
        //A.substring(0,1) and B.substring(0,1) selects the first char from each string and makes it uppercase by the help of .toUpperCase() function. 
        //The remaining string is taken as it is A.substring(1) (since the end index is not specified it takes the complete string)
        
        System.out.println(A.substring(0,1).toUpperCase()+A.substring(1) +" "+ B.substring(0,1).toUpperCase()+B.substring(1));
    }
}
